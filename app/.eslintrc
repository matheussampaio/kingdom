{
  "extends": [
    "airbnb/base",
    "angular"
  ],
  "env": {
    "es6": true,
    "jasmine": true,
    "browser": true
  },
  "rules": {
    "wrap-iife": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "padded-blocks": 0,
    "comma-dangle": [2, "never"],
    "spaced-comment": 0,
    "arrow-body-style": 0,
    "no-console": 0,
    "no-use-before-define": 0,
    "object-shorthand": 0,
    "quotes": [error, "backtick", "avoid-escape"]
  },
  "globals": {
    "inject": true
  }
}
